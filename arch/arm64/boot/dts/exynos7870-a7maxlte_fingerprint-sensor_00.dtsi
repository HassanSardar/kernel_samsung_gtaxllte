&btp_ldo &btp_check>;			pinctrl-1 = <&btp_irqsleep>;			pinctrl-2 = <&btp_irqidle>;			gpio-controller;			#gpio-cells = <2>;			vfsspi-sleepPin = <&gpd4 3 0x00>;			vfsspi-drdyPin = <&gpa0 6 0x00>;			vfsspi-ldoPin = <&gpd4 5 0x00>;			vfsspi-orient = <0>;			vfsspi-vendorPin = <&gpc7 4 0x00>;			vfsspi-ldocontrol = <1>;			vfsspi-csgpio = <&gpc7 1 0>;			controller-data {				samsung,spi-feedback-delay = <0>;				samsung,spi-chip-select-mode = <1>;			};		};		etspi-spi@1 {			compatible = "etspi,et320";			reg = <1>;			spi-max-frequency = <16000000>;			pinctrl-names = "default";			pinctrl-0 = <&btp_irq &btp_reset &btp_ldo>;			pinctrl-1 = <&btp_irqsleep>;			pinctrl-2 = <&btp_irqidle>;			gpio-controller;			#gpio-cells = <2>;			etspi-sleepPin = <&gpd4 3 0x00>;			etspi-drdyPin = <&gpa0 6 0x00>;			etspi-ldoPin = <&gpd4 5 0x00>;			etspi-csgpio = <&gpc7 1 0>;			controller-data {				samsung,spi-feedback-delay = <0>;				samsung,spi-chip-select-mode = <1>;			};		};	};};